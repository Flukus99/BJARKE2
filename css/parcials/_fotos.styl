.imagenes{
background-color:black;
display: grid;
grid-template-columns: 1fr 1fr 1fr 1fr;



}
.imagenes img{
  max-width:300px;
  height: 300px;
  border-radius: 50%;
  object-fit: cover;
  cursor: pointer;
  transition: .5s;
}


footer{
  background-color: black;
}

@media (max-width: 950px) {
.imagenes{
  grid-template-columns: 1fr 1fr 1fr;

}


}
@media (max-width: 875px){
  .imagenes{
    grid-template-columns: 1fr 1fr;
text-align: center;

  }

}
@media (max-width:650px) {
  .imagenes img{
    max-width: 200px;
    height: 200px;

  }
}
